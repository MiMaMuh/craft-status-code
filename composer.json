{
    "name": "squaresandbrackets/craft-status-code",
    "description": "Returns the HTTP status code within twig templates",
    "type": "craft-plugin",
    "version": "1.0.2",
    "keywords": ["craft", "cms", "craftcms", "craft-plugin", "status code"],
    "support": {
        "docs":
            "https://github.com/MiMaMuh/craft-status-code/",
        "issues":
            "https://github.com/MiMaMuh/craft-status-code/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Squares And Brackets",
            "homepage": "http://squaresandbrackets.com"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC9"
    },
    "autoload": {
        "psr-4": {
            "squaresandbrackets\\statuscode\\": "src/"
        }
    },
    "extra": {
        "name": "Status Code",
        "handle": "status-code",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl":
            "https://raw.githubusercontent.com/MiMaMuh/craft-status-code/master/CHANGELOG.md",
        "class": "squaresandbrackets\\statuscode\\StatusCode"
    }
}
